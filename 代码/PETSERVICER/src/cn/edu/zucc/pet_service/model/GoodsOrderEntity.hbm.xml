<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.zucc.pet_service.model.GoodsOrderEntity" table="goods_order" schema="pet_service">
        <id name="orderId" column="order_id">
            <generator class="increment"/>
        </id>
        <property name="orderNum" column="order_num"/>
        <property name="orderPrice" column="order_price"/>
        <property name="orderStatus" column="order_status"/>
        <set name="goods" table="goods_order_r">
            <key column="order_id"/>
            <many-to-many class="cn.edu.zucc.pet_service.model.GoodsEntity" column="goods_id"/>
        </set>
    </class>
</hibernate-mapping>